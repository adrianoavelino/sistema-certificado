    <script src="/templates/gentelella/vendors/flot.orderbars/js/jquery.flot.orderBars.js"></script>
    <script src="/templates/gentelella/vendors/flot-spline/js/jquery.flot.spline.min.js"></script>
    <script src="/templates/gentelella/vendors/flot.curvedlines/curvedLines.js"></script>
    <!-- DateJS -->
    <script src="/templates/gentelella/vendors/DateJS/build/date.js"></script>
    <!-- JQVMap -->
    <script src="/templates/gentelella/vendors/jqvmap/dist/jquery.vmap.js"></script>
    <script src="/templates/gentelella/vendors/jqvmap/dist/maps/jquery.vmap.world.js"></script>
    <script src="/templates/gentelella/vendors/jqvmap/examples/js/jquery.vmap.sampledata.js"></script>
    <!-- bootstrap-daterangepicker -->
    <script src="/templates/gentelella/vendors/moment/min/moment.min.js"></script>
    <script src="/templates/gentelella/vendors/bootstrap-daterangepicker/daterangepicker.js"></script>

    <!-- Custom Theme Scripts -->
    <script src="/templates/gentelella/build/js/custom.min.js"></script>
